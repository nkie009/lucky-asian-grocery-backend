<h1>Order Index Page</h1>


<table>
  <thead>
    <tr>
      <th>User Name</th>
      <th>Order Address</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |o| %>
      <tr>
        <td><%= o.user[:first_name]%></td>
        <td><%= o.address %></td>
        <td><%= o.status %></td>
        <td><%= link_to 'Show', order_path(o.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>


